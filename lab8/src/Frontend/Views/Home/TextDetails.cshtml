@{
    ViewData["Title"] = "Details";
}

<div>
    <h3>id: @(ViewData["id"])</h3>
    @if(ViewData["error"] != null)
    {
        <div>
		<p>Ошибка: @(ViewData["error"])</p>
	</div>    
    }
    else
    {
        <div>
		<p>Отношение: @(ViewData["rate"])</p>
	</div>
        <div>
		<p>Регион: @(ViewData["region"])</p>
	</div>
    }
</div>